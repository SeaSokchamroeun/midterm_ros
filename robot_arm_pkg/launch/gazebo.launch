<?xml version="1.0"?>
<launch>
	
		

		<include file="$(find gazebo_ros)/launch/empty_world.launch">
		</include>

		<param name="robot_description" command="$(find xacro)/xacro --inorder '$(find robot_arm_pkg)/urdf/slide_robot_arm.urdf'" />
		<node 
				name="spawn_robot" 
				pkg="gazebo_ros" 
				type="spawn_model" 
				respawn="false"
				args="-urdf -model myoboat -param robot_description -z 1">
		</node>
	
	


